<div class="dishes_container">
    <div class="cards">
      <% @dishes.each do |dish|  %>
        <div class="card">
          <div class="card_content">
            <h3><%= dish.name %></h3> by <%= dish.user.username %>
            <p><%= dish.description %></p>
          </div>
        </div>
      <% end %>
</div>
